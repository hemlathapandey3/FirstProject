<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
    <servlet-name>Registration</servlet-name>
    <servlet-class>msit.Controller.RegistrationController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Registration</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>msit.Controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProfileUpdate</servlet-name>
    <servlet-class>msit.Controller.ProfileController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProfileUpdate</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuickSearch</servlet-name>
    <servlet-class>msit.Controller.SearchController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuickSearch</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>help</servlet-name>
  <servlet-class>msit.Controller.HelpController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>help</servlet-name>
  <url-pattern>/help</url-pattern>
  
  </servlet-mapping>
  
  
  
  
  
</web-app>